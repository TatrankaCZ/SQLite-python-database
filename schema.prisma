// database
datasource db {
  provider = "sqlite"
  url      = "file:database.db"
}


generator client {
  provider = "prisma-client-js"
}


generator py {
  provider             = "prisma-client-py"
  recursive_type_depth = 5
  output = "./client"
}


model Room {
  id        Int     @id @default(autoincrement())
  guess_number Int
  score Int @default(100)
  min_number Int @default(1)
  max_number Int @default(11)
}
